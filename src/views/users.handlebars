<h1>User List</h1>

<h3>Active Users:</h3>
    <ul>
        {{#each users}}
        <li>
            
                <a href="/users/{{this._id}}" style="text-decoration: none;"><h3>{{this.firstName }} {{ this.lastName}}</h3></a>
                <p>Email: {{this.email}}</p>
                <p>Last Connection: {{this.lastConnection}}</p>
            
        </li>
        {{/each}}
    </ul>


<h3>Inactive Users</h3>
    <ul>
        {{#each inactiveUsers}}
        <li>

            <a href="/users/{{this._id}}" style="text-decoration: none;"><h3>{{this.firstName}} {{this.lastName}}</h3></a>
            <p>Email: {{this.email}}</p>
            <p>Last Connection: {{this.lastConnection}}</p>
        </li>
    {{/each}}
    </ul>